{
  "name": "fhevm-react-template",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK and React template for building privacy-preserving dApps",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "dev:next": "npm run dev --workspace=examples/nextjs-demo",
    "dev:react": "npm run dev --workspace=examples/react-demo",
    "dev:healthcare": "npm run dev --workspace=examples/healthcare-app",
    "build:all": "npm run build --workspaces",
    "compile:contracts": "cd examples/healthcare-app && npx hardhat compile",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "lint": "npm run lint --workspaces",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "privacy",
    "blockchain",
    "ethereum",
    "react",
    "nextjs",
    "sdk"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fhevm-react-template"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
